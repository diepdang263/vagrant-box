server {
	listen      1999;
	root        /vagrant/www;
	index       index.php index.html;
	server_name localhost;
	access_log  /var/log/nginx/admin-access.log;

	location / {
		try_files  $uri $uri/ =404;
	    expires    max;
	    access_log on;
	}

	location ~ /\.ht {
        deny all;
    }

    location ~ \.php$ {
        fastcgi_pass unix:/var/run/php/php7.1-fpm.sock;
        include fastcgi_params;
    }
}
